{
	"name":"gleeBox",
	"version":"1.6",
	"description":"Keyboard glee for your web",
	"icons":{
		"128": "images/icon_128.png"
	},
	"background_page":"background.html",
	"options_page":"options.html",	
	"content_scripts":[
	{
		"matches":["http://*/*","https://*/*"],
		"css":["css/glee.css","css/listmanager.css", "css/themes.css"],
		"js":["js/jquery.js","js/glee.js","js/chrome.js","js/page-cmds.js","js/utils.js","js/linkreaper.js","js/listmanager.js","js/esp.js"],
		"run_at":"document_start"
	}],
	"permissions":[
		"tabs",
		"http://api.bit.ly/",
		"bookmarks"
	],
	"browser_action":{
		"default_icon":"images/icon_19.png",
		"default_title":"Turn gleeBox OFF"
	}
}