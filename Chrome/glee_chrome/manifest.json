{
	"name": "gleeBox",
	"version": "1.7",
	"description": "Keyboard glee for your web",
	"background_page": "background.html",
	"options_page": "options.html",
	
	"icons": {
		"128": "images/icon_128.png"
	},
	
	"content_scripts": [
	{
		"matches":["http://*/*", "https://*/*"],
		
		"js": [
			"js/injectcss.js"
		],
		
		"run_at": "document_end",
		"all_frames": true
	},
	{
		"matches": [ "http://*/*", "https://*/*" ],
		
		"css": [
			"css/glee.css",
			"css/listmanager.css",
			"css/themes.css",
			"css/jquery.autocomplete.css"
		],
		
		"js": [
			"js/jquery.js",
			"js/glee.js",
			"js/events.js",
			"js/chrome.js",
			"js/page-cmds.js",
			"js/chrome-page-cmds.js",
			"js/utils.js",
			"js/linkreaper.js",
			"js/listmanager.js",
			"js/selectorGenerator.js",
			"js/jquery.autocomplete.js"
		],
		
		"run_at":"document_start"
	}],
	
	"permissions": [
		"tabs",
		"http://api.bit.ly/",
		"http://thegleebox.com/*",
		"bookmarks"
	]
}