{
	"name":"Glee",
	"version":"1.0",
	"description":"Keyboard Glee for your web",
	"icons":{
		"128": "images/icon_128.png"
	},
	"background_page":"background.html",
	"content_scripts":[
	{
		"matches":["http://*/*"],
		"css":["css/glee.css"],
		"js":["js/jquery.js","js/glee.js"],
		"run_at":"document_start"
	}],
	"permissions":[
		"tabs",
		"http://api.bit.ly/"
	],
	"browser_action":{
		"default_icon":"images/icon_19.png",
		"default_title":"Glee"
	}
}